{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "bashdb",
      "request": "launch",
      "name": "Bash-Debug (generic)",
      "program": "${file}"
    },
    {
      "type": "bashdb",
      "request": "launch",
      "name": "Bash-Debug (AwsDefaultConfig)",
      "program": "installScripts/AwsDefaultConfig",
      "args": []
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Convert VSCode Profiles",
      "program": "${workspaceFolder}/VsCodeConfig-profiles/convert-vscode-profiles.js",
      "args": [
        "${workspaceFolder}/VsCodeConfig-profiles",
        "${workspaceFolder}/VsCodeConfig-profiles/targetDirForDebug"
      ],
      "runtimeExecutable": "node",
      "cwd": "${workspaceFolder}/",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}

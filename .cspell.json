{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/bin/**",
    "**/vendor/**",
    "**/backup/**",
    "**/logs/**",
    "**/*megalinter_file_names_cspell.txt",
    ".history/**",
    ".cspell/**",
    ".vscode",
    "megalinter",
    "package-lock.json",
    "report",
    ".jscpd.json",
    ".mega-linter.yml",
    ".shellcheckrc",
    ".env"
  ],
  "noConfigSearch": true,
  "dictionaryDefinitions": [
    {
      "name": "bash",
      "path": ".cspell/bash.txt"
    },
    {
      "name": "config",
      "path": ".cspell/config.txt"
    },
    {
      "name": "softwares",
      "path": ".cspell/softwares.txt"
    },
    {
      "name": "readme",
      "path": ".cspell/readme.txt"
    },
    {
      "name": "dirColors",
      "path": ".cspell/dirColors.txt"
    }
  ],
  "dictionaries": ["bash", "config", "dirColors", "readme", "softwares"],
  "languageSettings": [
    {
      "languageId": "dirColors",
      "locale": "*",
      "dictionaries": ["dirColors"]
    },
    {
      "languageId": "bash",
      "locale": "*",
      "dictionaries": ["bash"]
    },
    {
      "languageId": "config",
      "locale": "*",
      "dictionaries": ["config"]
    },
    {
      "languageId": "softwares",
      "locale": "*",
      "dictionaries": ["softwares"]
    },
    {
      "languageId": "readme",
      "locale": "*",
      "dictionaries": ["readme"]
    }
  ],

  "patterns": [
    {
      "name": "urls",
      "pattern": "/https?://([^ \t\"'()]+)/g"
    },
    {
      "name": "packages",
      "pattern": "/[-A-Za-z0-9.]+/[-A-Za-z0-9.]+/g"
    },
    {
      "name": "markdownToc",
      "pattern": "\\]\\(#[^)]+\\)$"
    }
  ],
  "ignoreRegExpList": ["urls", "packages", "markdownToc"],
  "enableFiletypes": ["shellscript"],
  "words": [
    "Aftertabs",
    "Anacron",
    "Awscli",
    "Awsume",
    "Bazyli",
